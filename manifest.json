{
  "manifest_version": 2,
  "name": "Replace Donald Trump",
  "description": "This extension replaces the word Donald Trump with Stupid",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*:\/\/*\/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}